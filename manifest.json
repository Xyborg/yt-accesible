{
    "manifest_version": 3,
    "name": "YouTube Playlist Button Enhancer",
    "version": "1.3",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }